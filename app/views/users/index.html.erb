<% provide(:title, 'All users') %>
<h1>ユーザー一覧</h1>

    <%= form_tag({controller:"users",action:"index"}, method: :get) do %>
        <%= text_field_tag :search %>
        <%= submit_tag '検索する'  %>
    <% end %>
<%#%↑うまくいっていない%>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
    </li>
    <p><%= image_tag user.image_url, size: "250x200" if user.image? %></p>
  <% end %>
</ul>